<h1>Update Password<h1>

<%= form_for @user, url: "/admin/users/#{@user.id}/edit_password", method: :patch do |form| %>

  <%= form.label :password %>
  <%= form.text_field :password %>

  <%= form.label :password_confirmation %>
  <%= form.text_field :password_confirmation %>

  <%= form.submit "Update Info" %>

<% end %>
